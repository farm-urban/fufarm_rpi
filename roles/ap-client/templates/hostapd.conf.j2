# Set interface in ap.service systemd script
{% if external_wifi %}
interface={{ wifi_ap_interface }}
{% endif %}
ssid={{ ap_ssid }}
hw_mode=g
channel={{ ap_channel }}
driver=nl80211
wmm_enabled=0
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase={{ ap_password }}
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP
rsn_pairwise=CCMP
